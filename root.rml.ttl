@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ex: <http://example.com/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rmlt: <http://semweb.mmlab.be/ns/rml-target#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix formats: <http://www.w3.org/ns/formats/> .
@prefix tree: <https://w3id.org/tree#> .
@prefix elements: <http://purl.org/dc/elements/1.1/> .

@base <http://example.com/base/> .

<#TargetDump> a rmlt:LogicalTarget;
  rmlt:target [ a void:Dataset;
    void:dataDump <file://./root.ttl>;
  ];
  rmlt:serialization formats:Turtle;
.

<TriplesMap1>
  a rr:TriplesMap;
    
  rml:logicalSource [ 
    rml:source "list.csv";
    rml:referenceFormulation ql:CSV 
  ];

  rr:subjectMap [ 
    rr:template "https://treecg.github.io/Blue-Bike-to-OSLO/history/root.ttl";
    rml:logicalTarget <#TargetDump>;
  ]; 
	
  rr:predicateObjectMap [ 
    rr:predicate tree:relation; 
    rr:objectMap [
      rml:reference "file";
      rr:termType rr:BlankNode;
    ]
  ].

<TriplesMap2>
  a rr:TriplesMap;
    
  rml:logicalSource [ 
    rml:source "list.csv";
    rml:referenceFormulation ql:CSV 
  ];

  rr:subjectMap [ 
    rml:reference "file";
    rr:termType rr:BlankNode;
    rr:class tree:GreaterThanOrEqualToRelation;
    rml:logicalTarget <#TargetDump>;
  ]; 
	
  rr:predicateObjectMap [ 
    rr:predicate tree:path; 
    rr:object elements:date;
  ];

  rr:predicateObjectMap [ 
    rr:predicate tree:value; 
    rr:objectMap [ rml:reference "timestamp"; rr:datatype xsd:dateTime; ];
  ];

  rr:predicateObjectMap [ 
    rr:predicate tree:node; 
    rr:objectMap [
      rr:template "https://treecg.github.io/Blue-Bike-to-OSLO/history/{file}";
      rr:termType rr:IRI;
    ];
  ]
.
